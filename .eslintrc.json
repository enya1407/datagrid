{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "env": {
    "browser": true
    //    "jasmine": true,
    //    "jest": true
  },
  "rules": {
    "react/prop-types": 0
    //    "camelcase": "off",
    //    "@typescript-eslint/camelcase": 0
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  },
  "parser": "@typescript-eslint/parser"
}
